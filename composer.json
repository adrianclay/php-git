{
    "name": "adrianclay/git",
    "type": "library",
    "description": "Parse git repositories in native PHP",
    "keywords": [ "git", "native" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Clay",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-zlib": "*"
    },
    "require-dev": {
        "infection/infection": "^0.18",
        "mikey179/vfsstream": "1.6.12",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "adrianclay\\git\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "adrianclay\\git\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
