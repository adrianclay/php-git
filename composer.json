    "keywords": [ "git", "native" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Clay",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "webignition/url": "2.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.6.5",
        "phpunit/phpunit": "~4.8|~5.7"
    },
    "autoload": {
        "psr-4": {
            "adrianclay\\git\\": "src/"
        }
